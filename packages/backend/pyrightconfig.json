{
  "include": [
    "src",
    "tests"
  ],
  "extraPaths": [
    "src"
  ],
  "executionEnvironments": [
    {
      "root": "src"
    },
    {
      "root": "tests",
      "extraPaths": [
        "src"
      ]
    }
  ],
  "exclude": [
    ".venv",
    "**/__pycache__",
    "**/node_modules",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.ruff_cache"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "reportInvalidTypeForm": false
}